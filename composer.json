{
  "name": "ng-voice/asterisk_ari_amqp_lib",
  "type": "library",
  "description": "PHP library to build asterisk stasis applications in a microservice environment.",
  "keywords": ["ari", "asterisk", "rest", "stasis", "client", "stasisapp", "microservice"],
  "homepage": "https://bitbucket.org/lukas-ng-voice/asterisk_ari_amqp_lib",
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas Stermann",
      "email": "lukas@ng-voice.com",
      "homepage": "https://github.com/oktavlachs"
    },
    {
      "name": "Rick Barentin",
      "email": "rick@ng-voice.com",
      "homepage": "https://ng-voice.com"
    }
  ],
  "require" : {
    "php": ">=7.2.11",
    "monolog/monolog": "2.x-dev",
    "guzzlehttp/guzzle": "6.3.3",
    "php-amqplib/php-amqplib": "v2.8.0",
    "symfony/yaml": "v4.1.7",
    "nekland/woketo": "2.2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.4.3",
    "phpunit/php-code-coverage": "6.1.4"
  },
  "autoload": {
    "psr-4": {
      "AriStasisApp\\": "src/"
    },
    "files": ["src/helping_functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "AriStasisApp\\Tests\\": "tests/"
    }
  }
}
