{
  "name": "ng-voice/asterisk-ari-client",
  "type": "library",
  "description": "An object oriented client for the Asterisk REST Interface (ARI). Handles ARI calls and events for you.",
  "keywords": [
    "ari",
    "asterisk",
    "rest",
    "stasis",
    "client",
    "stasisapp",
    "telephone",
    "pbx"
  ],
  "homepage": "https://bitbucket.org/ngvoice/asterisk-ari-client",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Lukas Stermann",
      "email": "lukas@ng-voice.com",
      "homepage": "https://ng-voice.com",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": ">7.2",
    "monolog/monolog": "1.24.0",
    "guzzlehttp/guzzle": "6.3.3",
    "symfony/yaml": "v4.2.4",
    "nekland/woketo": "2.2.1",
    "netresearch/jsonmapper": "v1.4.0",
    "ext-http": "*",
    "ext-json": "*"
  },
  "suggest": {
    "ext-libevent": "Allows even better performance for the WebSocketClient"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8.0.5",
    "phpunit/php-code-coverage": "7.0.3"
  },
  "autoload": {
    "psr-4": {
      "NgVoice\\AriClient\\": "src/"
    },
    "files": [
      "src/helping_functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "NgVoice\\AriClient\\Tests\\": "tests/"
    },
    "files": [
      "src/helping_functions.php",
      "tests/shared_test_functions.php"
    ]
  },
  "scripts": {
    "test": [
      "phpenmod xdebug",
      "vendor/bin/phpunit",
      "phpdismod xdebug"
    ]
  }
}
