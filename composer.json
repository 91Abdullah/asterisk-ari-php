{
  "name": "ng-voice/asterisk-ari-client",
  "type": "library",
  "description": "An object oriented client for the Asterisk REST Interface (ARI). Handles ARI calls and events for you.",
  "keywords": [
    "ari",
    "asterisk",
    "rest",
    "stasis",
    "client",
    "stasisapp",
    "telephone",
    "pbx"
  ],
  "homepage": "https://bitbucket.org/ngvoice/asterisk-ari-client",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Lukas Stermann",
      "email": "lukas@ng-voice.com",
      "homepage": "https://ng-voice.com",
      "role": "Software Engineer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "monolog/monolog": "2.0.0",
    "guzzlehttp/guzzle": "6.3.3",
    "symfony/yaml": "v4.3.4",
    "nekland/woketo": "2.2.1",
    "netresearch/jsonmapper": "v1.6.0",
    "ext-json": "*",
    "ext-http": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "3.4.2",
    "phpunit/phpunit": "8.3.4",
    "phpunit/php-code-coverage": "7.0.7",
    "phpstan/phpstan": "0.11.15",
    "mockery/mockery": "1.2.3",
    "dg/bypass-finals": "1.1.2"
  },
  "autoload": {
    "psr-4": {
      "NgVoice\\AriClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NgVoice\\AriClient\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs -s",
    "lint-fix": "vendor/bin/phpcbf",
    "sca": "vendor/bin/phpstan analyse src tests --level 7",
    "test": "vendor/bin/phpunit"
  }
}
